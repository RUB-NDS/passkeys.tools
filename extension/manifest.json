{
    "name": "Passkey-Interceptor",
    "description": "Virtual authenticator intercepting all webauthn operations for testing purposes.",
    "version": "1.0.0",
    "manifest_version": 3,
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["content.js"],
            "run_at": "document_start",
            "all_frames": true
        }
    ],
    "action": {
        "default_popup": "popup.html"
    },
    "permissions": [
        "storage"
    ],
    "web_accessible_resources": [
        {
            "resources": ["init.js", "globals.js", "helpers.js", "hooks.js"],
            "matches": ["<all_urls>"]
        }
    ]
}
